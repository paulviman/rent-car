<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="228.0" prefWidth="985.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carrental.Controller.EmployeeController">
   <children>
      <BorderPane prefHeight="228.0" prefWidth="985.0" styleClass="car-card" stylesheets="@css/fullpackstyle.css">
         <top>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" styleClass="car-card-name" stylesheets="@css/fullpackstyle.css" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="employeName" alignment="CENTER" prefHeight="37.0" prefWidth="230.0">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </opaqueInsets>
            </HBox>
         </top>
         <left>
            <VBox prefHeight="100.0" prefWidth="0.0" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <HBox alignment="CENTER" prefHeight="128.0" prefWidth="1001.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" minWidth="-Infinity" prefHeight="130.0" prefWidth="130.0">
                     <children>
                        <Label text="Email:">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="employeEmail" alignment="CENTER" prefHeight="17.0" prefWidth="264.0" text="label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" minWidth="-Infinity" prefHeight="130.0" prefWidth="130.0">
                     <children>
                        <Label prefHeight="20.0" prefWidth="55.0" text="Phone:">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="employePhone" alignment="CENTER" prefHeight="17.0" prefWidth="123.0" text="label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" minWidth="-Infinity" prefHeight="130.0" prefWidth="130.0">
                     <children>
                        <Label text="Address">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="employeAddress" alignment="CENTER" prefHeight="17.0" prefWidth="264.0" text="label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" layoutX="394.0" layoutY="10.0" minWidth="-Infinity" prefHeight="130.0" prefWidth="130.0">
                     <children>
                        <Label text="Role">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="employeRole" alignment="CENTER" prefHeight="17.0" prefWidth="264.0" text="label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" layoutX="596.0" layoutY="10.0" minWidth="-Infinity" prefHeight="130.0" prefWidth="130.0">
                     <children>
                        <Label text="No. rents">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="employeNoRents" alignment="CENTER" prefHeight="17.0" prefWidth="264.0" text="label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" minWidth="-Infinity" prefHeight="128.0" prefWidth="130.0" spacing="15.0">
                     <children>
                        <Button fx:id="btnEditEmploye" mnemonicParsing="false" onAction="#actionBtnEditEmploye" prefHeight="38.0" prefWidth="77.0" styleClass="button-edit" stylesheets="@css/fullpackstyle.css" text="Edit" />
                        <Button fx:id="bntDeleteEmploye" mnemonicParsing="false" onAction="#actionBtnDeleteEmploye" styleClass="button-delete" text="Delete" />
                     </children>
                  </VBox>
               </children>
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
            </HBox>
         </center>
      </BorderPane>
   </children>
   <opaqueInsets>
      <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
   </opaqueInsets>
</AnchorPane>
